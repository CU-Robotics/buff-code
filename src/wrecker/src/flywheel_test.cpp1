#include <Arduino.h>
#include "drivers/flywheel.h"

flywheel fw1;
flywheel fw2;

Servo myServo;


void setup() {
    myServo.attach(28);
    Serial.begin(115200);
}

void loop() {
    if (Serial.available() > 2)
    {  
        delay(20);
        int temp = Serial.parseInt();
        myServo.attach(temp);
        Serial.print("new power: ");
        Serial.println(temp);
    }
    
}